--[[
Finds memrec by ID or description and tries to make it visible.
Adds menu subitem "Scroll to" under "Edit" menu item.
Available via &lt;Ctrl+F&gt; and &lt;Alt+E,S&gt; shortcuts.

Install by placing this file in CE autorun folder
by default "C:\Program Files (x86)\Cheat Engine 6.8.3\autorun"

Type part of a description and select one of the remaining items in the list below the searchbox.
Or press &lt;Enter&gt; to select first item right away.
To search by ID, paste it into searchbox and press &lt;Enter&gt;

Tested only on CE 6.8.3
Author: ametalon
Date: 2019-07-24
]]

-- namespace
amtScrollTo = {}

-- Recursively activates parents of a given memrec and selects it.
-- @param memrec MemoryRecord
function amtScrollTo:scrollToMemrec(memrec)
  if(not memrec) then return end
  local function recursive_activate(memrec)
    if(not memrec) then return end
    recursive_activate(memrec.Parent)
    memrec.Active = true
  end
  recursive_activate(memrec.Parent)
  -- focus AddressList's TTreeviewWithScroll to set keyboard focus for a selected memrec
  getAddressList().Control[0].setFocus()
  getAddressList().setSelectedRecord(memrec)
end

function amtScrollTo:findMemrec()
  local addr_list = getAddressList()

  if not self.description_list or self.description_list.Count ~= addr_list.Count then
    self:_refreshSearchList()
  end

  local i, text = showSelectionList("Scroll to memory record", "Enter ID or description\n&lt;Enter&gt; selects first element in the list", self.description_list, true)
  -- one of the list elements was selected
  if i &gt;= 0 then return addr_list[i] end
  -- no text was typed
  if text == '' then return end
  -- check if ID was typed
  local id = tonumber(text)
  if(id) then
    id = addr_list.getMemoryRecordByID(id)
  end
  return id or addr_list.getMemoryRecordByDescription(text) or self:_dumbSearch(text)
end

-- horrible, horrible searching. But it works
-- returns first memrec with description containing given text
function amtScrollTo:_dumbSearch(text)
  text = string.lower(text)
  for i = 0, self.description_list.Count-1 do
    -- plain "find substring" operation
  	if string.find(string.lower(self.description_list[i]), text, 1, true) then
  	  return getAddressList()[i]
  	end
  end
end

-- build data that will be used for a search
function amtScrollTo:_refreshSearchList()
  local addr_list = getAddressList()
  self.description_list = createStringlist()
  for i = 0, addr_list.Count-1 do
    self.description_list.add(addr_list[i].Description)
  end
end

-- add new menu item
-- if can't find Edit menu item add to the main menu itself
function addScrollToMemRec()
  if scrollToMemrecAdded then return end
    local parent = getMainForm().Edit3 or getMainForm().Menu.Items
    local scrolltomemrecitem = createMenuItem(parent)
    parent.add(scrolltomemrecitem)
    scrolltomemrecitem.Caption = '&amp;Scroll to'
    scrolltomemrecitem.Shortcut = "Ctrl+F"
    scrolltomemrecitem.OnClick = function() amtScrollTo:scrollToMemrec(amtScrollTo:findMemrec()) end
    scrollToMemrecAdded = 'True'
end
addScrollToMemRec()
